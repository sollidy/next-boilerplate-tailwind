{
  "semi": false,
  "singleQuote": true,
  "endOfLine": "auto",
  "importOrder": [
    "<THIRD_PARTY_MODULES>",
    "^@screens(.*)$",
    "^@ui(.*)$",
    "^@hooks(.*)$",
    "^@shared(.*)$",
    "^@services(.*)$",
    "^@assets(.*)$",
    "^@utils(.*)$",
    "^@config(.*)$",
    "^@store(.*)$",
    "^../(.*)",
    "^./(.*)",
    "(.scss)$"
  ],
  "plugins": ["prettier-plugin-tailwindcss"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
